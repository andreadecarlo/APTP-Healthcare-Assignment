(fill r1 b3 thermometer central_warehouse)
(pick-up r1 b3 central_warehouse c1 capacity_1 capacity_2)
(move r1 central_warehouse l2 c1)
(drop r1 b3 l2 u2 c1 capacity_1 capacity_2)
(empty r1 b3 thermometer u2 l2)
(move r1 l2 l1 c1)
(take-patient r2 p1 entrance)
(move-with-patient r2 p1 entrance l1)
(release-patient r2 p1 l1 u1)
(move-without-patient r2 l1 entrance)
(take-patient r2 p2 entrance)
(move-with-patient r2 p2 entrance l2)
(release-patient r2 p2 l2 u2)
(return-to-warehouse r1 l1 c1 capacity_0)
(fill r1 b2 bandage central_warehouse)
(pick-up r1 b2 central_warehouse c1 capacity_1 capacity_2)
(move r1 central_warehouse l1 c1)
(drop r1 b2 l1 u1 c1 capacity_1 capacity_2)
(empty r1 b2 bandage u1 l1)
(return-to-warehouse r1 l1 c1 capacity_0)
(fill r1 b1 scalpel central_warehouse)
(pick-up r1 b1 central_warehouse c1 capacity_1 capacity_2)
(move r1 central_warehouse l1 c1)
(drop r1 b1 l1 u1 c1 capacity_1 capacity_2)
(empty r1 b1 scalpel u1 l1)
; cost = 0 (general cost)
